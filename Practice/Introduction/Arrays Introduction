#include <cmath>
#include <iostream>
#include <math.h>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int a[1000], n;
    
    cin >> n;
    for (int i=0; i < n; i++) {
        cin >> a[i];
    }
    
    for (int i=n-1; i >= 0; i--) {
        cout << a[i] << " ";
    }
    return 0;
}
