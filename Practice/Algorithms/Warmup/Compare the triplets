#include <iostream>
#include<math.h>

using namespace std;

int main() {
    int x=0, y=0;
    int arr1[10], arr2[10];
    
    for (int i=0; i < 3; i++) {
        cin >> arr1[i];
    }
    for (int i=0; i < 3; i++) {
        cin >> arr2[i];
    }
    
    for (int i=0; i < 3; i++) {
        if (arr1[i] > arr2[i]) {
            x++;
        }
        else if (arr2[i] > arr1[i]) {
            y++;
        }
    }
    cout << x << " " << y << endl;
    return 0;
}
